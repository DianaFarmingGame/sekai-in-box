; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="sekai-in-box"
run/main_scene="res://test/test.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
LisperDebugger="*res://addons/lisper-debugger/lisper_debugger.tscn"
sekai="*res://lib/sekai/sekai.gd"

[debug]

gdscript/warnings/integer_division=0
gdscript/warnings/confusable_local_declaration=0

[display]

window/size/viewport_width=576
window/size/viewport_height=640
window/subwindows/embed_subwindows=false
window/vsync/vsync_mode=2
mouse_cursor/custom_image="res://test/assert/ui/鼠标箭头.png"
window/defaults/default_clear_color=Color(0, 0, 0, 1)

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/lisper-debugger/plugin.cfg", "res://addons/sekai/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/lcd_subpixel_layout=2

[input]

combo={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":90,"key_label":0,"unicode":122,"echo":false,"script":null)
]
}
dialog_confirm={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"echo":false,"script":null)
]
}

[input_devices]

buffering/agile_event_flushing=true

[lisper]

enable_aggressive_opt=true
debugger_visible=true

[rendering]

environment/ssao/quality=0
environment/ssil/quality=0
anti_aliasing/screen_space_roughness_limiter/enabled=false
environment/defaults/default_clear_color=Color(0, 0, 0, 1)

[sekai]

debug_draw_collisible=false
debug_draw_routable=false
debug_draw_solid=false
debug_draw_contactable=false
debug_draw_pickable=false
define_entry=""
render_extra_sight=4
min_inits_per_frame=8
gikou_store_dir="user://gikous"
