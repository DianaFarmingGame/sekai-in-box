[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 =     "./rustlib/target/x86_64-unknown-linux-gnu/release/libgisp.so"
linux.release.x86_64 =   "./rustlib/target/x86_64-unknown-linux-gnu/release/libgisp.so"
windows.debug.x86_64 =   "./rustlib/target/x86_64-pc-windows-gnu/release/gisp.dll"
windows.release.x86_64 = "./rustlib/target/x86_64-pc-windows-gnu/release/gisp.dll"
macos.debug.arm64 =      "./rustlib/target/aarch64-apple-darwin/release/libgisp.dylib"
macos.release.arm64 =    "./rustlib/target/aarch64-apple-darwin/release/libgisp.dylib"
macos.debug =            "./rustlib/target/aarch64-apple-darwin/release/libgisp.dylib"
macos.release =          "./rustlib/target/aarch64-apple-darwin/release/libgisp.dylib"
